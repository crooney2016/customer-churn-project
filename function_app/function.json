[
  {
    "scriptFile": "__init__.py",
    "bindings": [
      {
        "name": "blob",
        "type": "blobTrigger",
        "direction": "in",
        "path": "churn-feature-data/{name}.csv",
        "connection": "AzureWebJobsStorage"
      }
    ]
  },
  {
    "scriptFile": "__init__.py",
    "bindings": [
      {
        "name": "timer",
        "type": "timerTrigger",
        "direction": "in",
        "schedule": "0 0 6 1 * *"
      }
    ]
  },
  {
    "scriptFile": "__init__.py",
    "bindings": [
      {
        "name": "req",
        "type": "httpTrigger",
        "direction": "in",
        "authLevel": "function",
        "methods": ["post"],
        "route": "score"
      }
    ]
  },
  {
    "scriptFile": "__init__.py",
    "bindings": [
      {
        "name": "req",
        "type": "httpTrigger",
        "direction": "in",
        "authLevel": "function",
        "methods": ["get"],
        "route": "health"
      }
    ]
  }
]
